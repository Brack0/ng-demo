{
  "name": "ng-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr --host 0.0.0.0 --disable-host-check",
    "build": "ng build core && ng build ng-demo",
    "watch": "ng build --watch --configuration development",
